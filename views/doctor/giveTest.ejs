  <div class="container">
    <div class="card card-body border-light shadow mt-4">
      <div class="row">
        <div class="col">
          <h1 class="text-center mb-3">
            <i class="fas fa-book-medical"></i> Tahlil Ver
          </h1>
  
          <form action="/doctor/giveTest" method="POST">
            <div class="form-group">
                <!--Hasta TC-->
                <label class="my-1 mr-2" for="hastaTC">Hasta TC</label>
                <input name="hastaTC" type="text" id="hastaTC" class="form-control">
            </div>
            <div class="form-group">
                <!--polyclinic name-->
                <label class="my-1 mr-2" for="clinics">Klinikler</label>
                <select
                  name="district"
                  class="custom-select my-1 mr-sm-2"
                  id="clinikler"
                >
                <% clinics.forEach((item)=> { %>
                  <option value= <%=item.polyclinicName%> > <%=item.polyclinicName%> </option>
                <% }) %>  
                </select>
            </div>
            <div class="form-group">
                <!--test name-->
                <label class="my-1 mr-2" for="testName">Tahlil Adı</label>
                <input name="testName" type="text" id="testName" class="form-control">
            </div>
            <div class="form-group">
                <!--date time-->
                <label class="my-1 mr-2" for="date">Tarih</label>
                <input name="date" type="text" id="datePicker" class="form-control">
            </div>

            <button onclick="sweetalertClick()" type="submit" class="btn btn-primary btn-block my-1">
              Tahlili Kaydet
            </button>
          </form>
        </div>
      </div>
    </div>
    <div class="text-center fa-xs">
      
    </div>
  </div>
  <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
